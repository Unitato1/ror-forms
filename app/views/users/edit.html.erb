<% @user.errors.full_messages.each do |error| %>
  <div><%= error %></div>
<% end %>
<%= form_with model: @user, method: :patch do |form| %>
  <%= form.label :username, "Username: "%>
  <%= form.text_field :username, placeholder: "Bob" %>
  <br>
  <%= form.label :password, "Password: "%>
  <%= form.password_field :password, placeholder: "!ex@!x"%>
  <br>
  <%= form.label :email, "Email: "%>
  <%= form.email_field :email, placeholder: "Bobs@email.com" %>
  <br>
  <%= form.submit "Edit!"%>
<% end %>